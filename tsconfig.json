{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@componentes/*": [
        "./src/componentes/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
